{
  "name": "babel-node-server-react",
  "main": "server/app.js",
  "typings": "server/app.d.ts",
  "scripts": {
    "postinstall": "npm run build",
    "build": "tsc --outDir server",
    "build-js": "tsc --outFile public/js/bundle.js assets/js/index.tsx",
    "build-server": "tsc --outDir server",
    "watch": "npm run watch-js && npm run watch-server",
    "watch-js": "tsc -w",
    "watch-server": "tsc -w --outDir server",
    "livereload": "nodemon server/app.js",
    "start": "node server/app.js"
  },
  "dependencies": {
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "nodemon": "^1.9.1",
    "typescript": "^1.8.7"
  }
}
