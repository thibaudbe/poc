{
  "name": "babel",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "npm run build",
    "build": "babel src -d dist",
    "start": "nodemon src/app.js --exec babel-node --presets es2015,stage-2",
    "serve": "node dist/app.js"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babelify": "7.2.0"
  }
}
