{
  "name": "babel",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "npm run build",
    "build": "babel src -o dist/compiled-app.js",
    "start-dev": "babel-node src/app.js -s",
    "start": "node dist/app.js",
    "watch": "babel src -w -o dist/compiled-app.js"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "babel-cli": "6.5.0",
    "babelify": "7.2.0"
  }
}
